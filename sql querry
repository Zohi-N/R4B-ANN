 DROP TABLE IF EXISTS oddaja_naloge CASCADE;
DROP TABLE IF EXISTS domača_naloga CASCADE;
DROP TABLE IF EXISTS gradiva CASCADE;
DROP TABLE IF EXISTS ocena CASCADE;
DROP TABLE IF EXISTS dijak_predmet CASCADE;
DROP TABLE IF EXISTS profesor_predmet CASCADE;
DROP TABLE IF EXISTS dijaki CASCADE;
DROP TABLE IF EXISTS razred CASCADE;
DROP TABLE IF EXISTS predmet CASCADE;
DROP TABLE IF EXISTS profesorji CASCADE;

CREATE TABLE profesorji (
    profesor_id SERIAL PRIMARY KEY,
    ime VARCHAR(255) NOT NULL,
    priimek VARCHAR(255) NOT NULL,
    gmail VARCHAR(255) NOT NULL UNIQUE,
    geslo VARCHAR(255) NOT NULL
);

CREATE TABLE razred (
    razred_id SERIAL PRIMARY KEY,
    oznaka VARCHAR(255) NOT NULL,
    smer VARCHAR(255) NOT NULL,
    profesor_id INT NOT NULL,
    FOREIGN KEY (profesor_id) REFERENCES profesorji(profesor_id) ON DELETE CASCADE
);

CREATE TABLE predmet (
    predmet_id SERIAL PRIMARY KEY,
    ime_predmeta VARCHAR(255) NOT NULL
);

CREATE TABLE dijaki (
    id_dijaka SERIAL PRIMARY KEY,
    ime_dijaka VARCHAR(255) NOT NULL,
    priimek_dijaka VARCHAR(255) NOT NULL,
    razred VARCHAR(255) NOT NULL,
    gmail VARCHAR(255) NOT NULL UNIQUE,
    geslo VARCHAR(255) NOT NULL,
    razred_id INT NOT NULL,
    FOREIGN KEY (razred_id) REFERENCES razred(razred_id) ON DELETE CASCADE
);

CREATE TABLE profesor_predmet (
    profesor_id INT NOT NULL,
    predmet_id INT NOT NULL,
    PRIMARY KEY (profesor_id, predmet_id),
    FOREIGN KEY (profesor_id) REFERENCES profesorji(profesor_id) ON DELETE CASCADE,
    FOREIGN KEY (predmet_id) REFERENCES predmet(predmet_id) ON DELETE CASCADE
);

CREATE TABLE dijak_predmet (
    id_dijaka INT NOT NULL,
    predmet_id INT NOT NULL,
    PRIMARY KEY (id_dijaka, predmet_id),
    FOREIGN KEY (id_dijaka) REFERENCES dijaki(id_dijaka) ON DELETE CASCADE,
    FOREIGN KEY (predmet_id) REFERENCES predmet(predmet_id) ON DELETE CASCADE
);

CREATE TABLE ocena (
    ocena_id SERIAL PRIMARY KEY,
    id_dijaka INT NOT NULL,
    predmet_id INT NOT NULL,
    vrednost INT NOT NULL CHECK (vrednost BETWEEN 1 AND 5),
    datum TIMESTAMP NOT NULL,
    FOREIGN KEY (id_dijaka) REFERENCES dijaki(id_dijaka) ON DELETE CASCADE,
    FOREIGN KEY (predmet_id) REFERENCES predmet(predmet_id) ON DELETE CASCADE
);

CREATE TABLE domača_naloga (
    naloga_id SERIAL PRIMARY KEY,
    predmet_id INT NOT NULL,
    naslov VARCHAR(255) NOT NULL,
    navodila TEXT NOT NULL,
    datum_objave TIMESTAMP NOT NULL,
    rok TIMESTAMP NOT NULL,
    FOREIGN KEY (predmet_id) REFERENCES predmet(predmet_id) ON DELETE CASCADE
);

CREATE TABLE oddaja_naloge (
    oddaja_id SERIAL PRIMARY KEY,
    naloga_id INT NOT NULL,
    id_dijaka INT NOT NULL,
    datum_objave TIMESTAMP NOT NULL,
    datoteka_link VARCHAR(255) NOT NULL,
    FOREIGN KEY (naloga_id) REFERENCES domača_naloga(naloga_id) ON DELETE CASCADE,
    FOREIGN KEY (id_dijaka) REFERENCES dijaki(id_dijaka) ON DELETE CASCADE
);

CREATE TABLE gradiva (
    gradivo_id SERIAL PRIMARY KEY,
    predmet_id INT NOT NULL,
    naslov VARCHAR(255) NOT NULL,
    opis TEXT,
    datoteka_link VARCHAR(255) NOT NULL,
    datum_objave TIMESTAMP NOT NULL,
    profesor_id INT NOT NULL,
    FOREIGN KEY (predmet_id) REFERENCES predmet(predmet_id) ON DELETE CASCADE,
    FOREIGN KEY (profesor_id) REFERENCES profesorji(profesor_id) ON DELETE CASCADE
);

